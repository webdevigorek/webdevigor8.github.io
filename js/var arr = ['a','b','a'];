var arr = ['a','b','a'];
function unique(arr){
	for (var i = 0; i < arr.length; i++){
	var non_exclusive = false;
	for (var j = 0; j < arr.length; j++){
	if (arr[i] == arr[j] && i != j){
	non_exclusive = true;
	break;
	} 
	}
	if(!non_exclusive){
	return arr[i];
	}
}
}
console.log(unique(arr));